public class SubsetsOfString {
    public static void printSubsets(String string) {
        int n = string.length();
        for (int i = 0; i < n; i++) {
            for (int j = i; j < n; j++) {
                System.out.println(string.substring(i, j + 1));
            }
        }
    }
    
    public static void main(String[] args) {
        String string = "FUN";
        printSubsets(string);
    }
}
